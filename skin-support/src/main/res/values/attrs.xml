    </declare-styleable>
    <declare-styleable name="SkinCompoundButtonHelper">
        <attr name="android:button" />
        <attr name="android:buttonTint" />
    </declare-styleable>
    <declare-styleable name="SkinSwitchHelper">
        <attr name="android:switchTextAppearance" />
        <attr name="android:thumb" />
        <attr name="android:thumbTint" />
        <attr name="android:track" />
