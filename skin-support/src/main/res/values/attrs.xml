        <attr name="android:drawableStart" />
        <attr name="android:drawableEnd" />
        <attr name="android:drawableTint" />
    </declare-styleable>
    <declare-styleable name="SkinAutoCompleteTextViewHelper">
        <attr name="android:popupTheme" />
